    <MakeDir Directories="$(SolutionDir)dist\$(PackageName)\extra" />

    <CallTarget Condition="'$(Configuration)' == 'Debug'" Targets="PackageDeploy"/>
  </Target>

  <Target Name="PackageDeploy">
    <ItemGroup>
      <SourcePackage Include="$(SolutionDir)dist\$(PackageName)\**\*" />
    </ItemGroup>
    <PropertyGroup>
      <DynamoVersion>2.0</DynamoVersion>
      <!--Copy to dynamo sandbox for testing -->
      <DynamoCore Condition="'$(Configuration)' == 'Debug'">$(AppData)\Dynamo\Dynamo Core\$(DynamoVersion)\packages\z_$(PackageName)</DynamoCore>
      <DynamoRevit Condition="'$(Configuration)' == 'Debug'">$(AppData)\Dynamo\Dynamo Revit\$(DynamoVersion)\packages\z_$(PackageName)</DynamoRevit>
    </PropertyGroup>
    <Copy SourceFiles="@(SourcePackage)" DestinationFolder="$(DynamoCore)\%(RecursiveDir)" />
    <Copy SourceFiles="@(SourcePackage)" DestinationFolder="$(DynamoRevit)\%(RecursiveDir)" />
    <Message Importance="High" Text="+++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++" />
    <Message Importance="High" Text="PACKAGE DEPLOYED TO $(DynamoCore) AND $(DynamoRevit)" />
  </Target>

